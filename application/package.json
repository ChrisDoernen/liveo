    "test:app:admin": "ng test admin",
    "test:app:client": "ng test client",
    "test:app:server": "ng test server",
    "apitests": "node_modules/.bin/newman run apps/server-apitests/liveo-server-apitests-postman-collection.json -e apps/server-apitests/localhost-3000.postman-environment.json",
