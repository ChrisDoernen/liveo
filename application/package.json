    "ng": "ng",
    "start": "node dist/apps/server/liveo-server.js",
    "start:app:admin": "ng serve admin --port 4200 --host 0.0.0.0",
    "start:app:client": "ng serve client --port 4200 --host 0.0.0.0",
    "start:app:server": "ng serve server",
    "clean": "rimraf dist",
